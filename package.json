    "msnodesqlv8": "^0.6.12",
    "mssql": "^5.0.5",
    "react": "^16.5.2",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-select": "^2.4.2",
    "redux": "^4.0.1"
  },
